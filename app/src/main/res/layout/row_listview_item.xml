    android:layout_height="wrap_content"
    android:id="@+id/tv_protperty_status_property"
    android:visibility="gone"/>




<RelativeLayout
    android:id="@+id/rl_list_item"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="5dp">


  <CheckBox
      android:id="@+id/check_fav"
        android:layout_width="34dp"
        android:layout_height="26dp"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="4dp"
        android:button="@drawable/check_box_selector"
      android:visibility="gone"/>


    <ImageView
        android:id="@+id/image"
        android:layout_width="86dp"
        android:layout_height="86dp"
        android:scaleType="fitXY"
        android:src="@drawable/default_image"
        android:layout_marginTop="4dp"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="4dp"
        android:layout_marginRight="4dp"/>




    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_property_title"
        android:text="Property Title"
        android:textSize="18dp"
        android:maxLength="15"
        android:textStyle="bold"
        android:layout_toRightOf="@+id/image"
        android:textColor="@color/colorWhite"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="10dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_dot_dot"
        android:text="...."
        android:textSize="20dp"
        android:layout_toRightOf="@+id/tv_property_title"
        android:textColor="@color/colorWhite"
        android:layout_marginTop="10dp"/>

